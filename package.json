{
  "name": "wavefont",
  "version": "2.0.0",
  "description": "Typeface for visualizing waveform data",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-values-1-0 && npm run build-values-1-1 && npm run build-values-1-center && npm run build-values-100-0 && npm run build-values-100-1 && npm run build-ttf",
    "build-values-1-0": "build-values.sh 1 0",
    "build-values-1-1": "build-values.sh 1 1",
    "build-values-1-center": "build-values-center.sh 1",
    "build-values-100-0": "build-values.sh 100 0",
    "build-values-100-1": "build-values.sh 100 1",
    "build-masterotfs": "buildmasterotfs -d masters/wavefont.designspace",
    "build-otf": "npm run build-masterotfs && buildcff2vf -c -k -d masters/wavefont.designspace -o ./font/wavefont.otf",
    "build-ttf": "fontmake -m masters/wavefont.designspace -o variable --output-path ./font/wavefont.ttf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/audio-lab/wavefont.git"
  },
  "keywords": [
    "sound",
    "audio",
    "datavis",
    "opentype",
    "waveform",
    "spectrum",
    "fft"
  ],
  "author": "Dmitry Iv <dfcreative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/audio-lab/wavefont/issues"
  },
  "homepage": "https://github.com/audio-lab/wavefont#readme",
  "devDependencies": {},
  "dependencies": {}
}
