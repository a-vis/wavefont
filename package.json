{
  "name": "wavefont",
  "version": "2.1.0",
  "description": "Typeface for visualizing waveform data",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ufo && npm run build-masterotfs & npm run build-otf && npm run build-ttf",
    "build-ufo": "plop build-ufo",
    "build-masterotfs": "buildmasterotfs --mkot -r,-omitMacNames -d masters/wavefont.designspace",
    "build-otf": "npm run build-masterotfs && buildcff2vf -c -k -d masters/wavefont.designspace -o ./font/wavefont.otf",
    "build-ttf": "fontmake --no-optimize-gvar -m masters/wavefont.designspace -o variable --output-path ./font/wavefont.ttf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-vis/wavefont.git"
  },
  "keywords": [
    "sound",
    "audio",
    "datavis",
    "opentype",
    "waveform",
    "spectrum",
    "fft"
  ],
  "author": "Dmitry Iv <dfcreative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-vis/wavefont/issues"
  },
  "homepage": "https://github.com/a-vis/wavefont#readme",
  "devDependencies": {
    "plop": "^2.7.4"
  },
  "dependencies": {}
}
